{
  "env": {
    "node": true,
    "es6": true
  },
  "parserOptions": {
    "ecmaVersion": 8
  },
  "rules": {
    "semi": 2,
    "quotes": [2, "single"],
    "comma-dangle": 0,
    "no-process-exit": 0,
    "func-names": 2,
    "space-before-blocks": 2,
    "brace-style": [2, "1tbs"],
    "keyword-spacing": 2,
    "space-before-function-paren": [2, "never"],
    "no-spaced-func": 2
  }
}
